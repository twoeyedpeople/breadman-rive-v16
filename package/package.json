{
  "name": "@mascotbot-sdk/react",
  "version": "0.1.6",
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist/**"
  ],
  "peerDependencies": {
    "@rive-app/react-webgl2": "^4.6.2",
    "@rive-app/webgl2": "^2.9.2",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@breezystack/lamejs": "^1.2.7",
    "@changesets/cli": "^2.27.9",
    "@pipecat-ai/client-js": "0.3.2",
    "@pipecat-ai/client-react": "0.3.2",
    "@pipecat-ai/daily-transport": "0.3.4",
    "@rive-app/react-webgl2": "^4.21.1",
    "@rive-app/webgl2": "^2.29.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.298.0",
    "motion": "^11.18.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.24",
    "@types/react": "18.3.1",
    "autoprefixer": "^10.4.20",
    "javascript-obfuscator": "^4.1.1",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup-plugin-obfuscator": "^1.1.0",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.4.14",
    "tsup": "^6.1.3",
    "typescript": "^5.1.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "tsup",
    "build:obfuscate": "tsup",
    "build:no-obfuscate": "DISABLE_OBFUSCATION=true tsup",
    "dev": "tsup --watch",
    "pack:no-obfuscate": "DISABLE_OBFUSCATION=true pnpm pack",
    "lint": "eslint .",
    "type-check": "tsc --pretty --noEmit",
    "type-check:ci": "tsc-absolute --pretty --noEmit",
    "lint:fix": "eslint . --fix",
    "lint:report": "eslint . --format json --output-file ../../lint-results/ui.json"
  }
}